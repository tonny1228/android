<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="works.tonny.mobile" android:versionName="1.0.1">

    <application
        android:allowBackup="true"
        android:label="@string/app_name">

        <activity
            android:name=".widget.WebViewActivity"
            android:label="@string/title_activity_web_view"
            android:launchMode="singleTask">
            <intent-filter>
                <action android:name="works.tonny.mobile.widget.WebViewActivity.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".widget.CameraActivity"
            android:label="@string/title_activity_camery"></activity>

        <service android:name=".service.UpdateService"></service>


    </application>

</manifest>
